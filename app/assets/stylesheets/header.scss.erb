.header {
  border-bottom: 1px solid #ccc;
  padding: 12px 120px;
  
  .links {
    padding-top: 27px;
    font-weight: bold;
    a {
      margin-left: 24px;
      text-transform: uppercase;
    }
  }  
}

.header_image {
  height: 309px;
  background: url(<%= image_path 'header.jpg' %>) no-repeat center center;  
}
