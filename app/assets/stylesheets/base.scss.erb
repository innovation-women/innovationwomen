$blue: #003a79;
$red: #ec3936;
$text: #484848;

body {
  padding: 0;
  margin: 0;
  color: $text;
}

a {
  color: #14273F;
  text-decoration: none;
  
  &:hover {
    color: #14273F;
    text-decoration: underline;
    background-color: white;
  }
}

.right {
  float: right;
}

.small {
  font-size: 11px;
}

.content {
  p, td {    
    font-size: 14px;
  }
}